<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.ac.hannam.multi.cricket.common.mapper.AdminMapper">
    <select id="selectAdmin" resultType="kr.ac.hannam.multi.cricket.vo.AdminVO">
        SELECT ADMIN_NO
               , ADMIN_EMAIL
               , ADMIN_PASSWORD
               , ADMIN_NAME
               , ADMIN_PHONE
          FROM ADMIN
         WHERE ADMIN_EMAIL=#{username}
    </select>
</mapper>